<Window x:Class="testApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:testApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="mainScreen">
        <Button x:Name="AddSongsButton" Content="Add Songs" HorizontalAlignment="Left" Height="56" Margin="627,273,0,0" VerticalAlignment="Top" Width="98" Click="AddSongsButton_Click"/>
        <ListBox x:Name="SongList" HorizontalAlignment="Left" Height="164" Margin="603,87,0,0" VerticalAlignment="Top" Width="145" SelectionChanged="SongList_SelectionChanged"/>
        <Button x:Name="NextSongButton" Content="Next &gt;&gt;" Margin="416,291,285,90" IsCancel="True" Click="NextSongButton_Click"/>
        <Button x:Name="PreviousSongButton" Content="&lt;&lt; Previous" Margin="96,291,605,90" IsCancel="True" Click="PreviousSongButton_Click"/>
        <Button x:Name="ShuffleSongsButton" Content="Shuffle" Margin="256,291,445,90" IsCancel="True" Click="ShuffleSongsButton_Click"/>
    </Grid>


</Window>
